<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="luma test">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="os" value="windows"/>
    <parameter name="osVersion" value="10"/>
    <parameter name="browserVersion" value="110"/>

    <parameter name="browserName" value="chrome"/>
    <parameter name="url" value="https://magento.softwaretestingboard.com/"/>
    <test name="luma Tests">
        <classes>
            <class name="luma.RegisterTest"/>
            <class name="luma.LoginTest"/>
            <class name="luma.CartTest"/>
            <class name="luma.SearchTest"/>
            <class name="luma.ProductsTest"/>
            <class name="luma.WishListTest"/>
            <class name="luma.MyAccountTest"/>
            <class name="luma.OrderTest"/>
        </classes>
    </test>
</suite>