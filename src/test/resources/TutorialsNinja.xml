<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="tutorialsNinja test">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserVersion" value="112"></parameter>

    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://tutorialsninja.com/demo/index.php?route=account/login"></parameter>

    <test name=" LoginTest">
        <classes>
            <class name="tutorialsNinja.LoginTest"></class>
        </classes>
    </test>
    <test name=" RegisterTest">
        <classes>
            <class name="tutorialsNinja.RegisterTest"></class>
        </classes>
    </test>


</suite>
