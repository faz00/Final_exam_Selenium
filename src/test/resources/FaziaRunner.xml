<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Gumtree test">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="110"></parameter>

    <parameter name="browserName" value="firefox"></parameter>
    <parameter name="url" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"></parameter>
    <test name="login test">
        <classes>
            <class name="Fazia.LoginTestOrange"></class>
        </classes>
    </test>
    <test name="logout test">
        <classes>
            <class name="Fazia.LogOutTest"></class>
        </classes>
    </test>
    <test name="ChangePassword test">
        <classes>
            <class name="Fazia.ChangePassword"></class>
        </classes>
    </test>

</suite>